package com.bridgeit.interfaceJava8;

public interface A {

	void add();
	
	//Not possible for default method
	/*public default boolean equals(Object object){
		return true;
	}*/
	
	//Static method can be defined inside interface in Java8 and cannot declare it to be static 
	static void show(){
		System.out.println("Defination of Static method inside interface..");
	}
	
	default void showDefault(){
		System.out.println("Defination of Default method inside interface..");
	}
}
